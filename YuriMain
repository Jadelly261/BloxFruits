-- Button Functionality
getkey.MouseButton1Click:Connect(function()
    setclipboard("https://link-target.net/1346847/exorcist-key-system1") -- Replace with your key link
    TextLabel.Text = "Key Link Copied!"
    wait(2)
    TextLabel.Text = "Yuri Loader"
end)

getdiscord.MouseButton1Click:Connect(function()
    setclipboard("https://discord.gg/KuV8HPNmCj") -- Replace with your Discord link
    TextLabel.Text = "Discord Link Copied!"
    wait(2)
    TextLabel.Text = "Yuri Loader"
end)

confirm.MouseButton1Click:Connect(function()
    if keyhere.Text == "PYUS-KEY" then
        TextLabel.Text = "Key Verified!"
        wait(2)
        ZENHUB:Destroy()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/Fiend1sh/FiendMain/refs/heads/main/YuriHub", true))()
    else
        TextLabel.Text = "Invalid Key. Try Again."
        wait(2)
        TextLabel.Text = "Yuri Loader"
    end
end)

X.MouseButton1Click:Connect(function()
    MAIN:Destroy()
end)
