local function roundify(instance, radius)
    local corner = Instance.new("UICorner")
    corner.CornerRadius = UDim.new(0, radius)
    corner.Parent = instance
end

-- Apply rounded corners
roundify(MAIN, 6)
roundify(keyhere, 6)
roundify(getkey, 6)
roundify(confirm, 6)
roundify(X, 6)

-- Connect button click events
getkey.MouseButton1Click:Connect(function()
    setclipboard("https://direct-link.net/615790/fuae-hub-key-system")
    DiscordLib:Notification("", "Paste on Your Browser", 3)
end)

confirm.MouseButton1Click:Connect(function()
    if keyhere.Text == "KEY-FUSE" then
        DiscordLib:Notification("", "Key Check, Executing Script...", 3)
        wait(3)
        ZENHUB:Destroy()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/Fiend1sh/FiendMain/refs/heads/main/FuseHub.lua", true))()
    else
        DiscordLib:Notification("", "Key Wrong Try Again", 3)
    end
end)

X.MouseButton1Click:Connect(function()
    MAIN:Destroy()
end)
